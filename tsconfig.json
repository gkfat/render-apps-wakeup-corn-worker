{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "lib": ["WebWorker", "ES2020"],
      "moduleResolution": "Node",
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true
    },
    "include": ["src"]
  }